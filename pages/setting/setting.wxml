    <button class="permission" open-type="openSetting" hover-class="btn_hover" style="margin:15px 0 0 0;padding:0;width: 100%;">
      <view class="title">设置</view>
      <view class="info">如果不能查询请点击这里,允许使用地理位置</view>      
    </button>
    <view class="clear">清楚缓存</view>
  </view>
  <button class="share" hover-class="btn_hover" open-type="share" style="margin:20px 0 0 0;padding:0;width: 100%;">
    <view class='title'>分享</view>
  </button>
  </view>