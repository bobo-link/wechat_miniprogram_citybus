/* pages/feedback_history/feedback_history.wxss */
.content{
  margin: 0 10px;
  margin-top:20px ;
}
.item{
  padding: 10px 0 20px 10px;
  border-radius:5px ;
  background-color: white;
  margin-bottom:10px ;
  position: relative;
}
.time{
  color: #aaa;
  font-size: 11px;
}
.status{
  position:absolute;
  bottom: 5px;
  right: 10px;
  width: 8px;
  height: 8px;
  border-radius: 100%;
  background: rgb(8, 250, 0);
}
.read{
  background: rgb(253, 125, 5);
}
.reply{
  background: rgb(139, 138, 138);
}