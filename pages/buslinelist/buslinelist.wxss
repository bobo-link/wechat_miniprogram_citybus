/* pages/buslinelist/buslinelist.wxss */
.busline--list{
  display: flex;
  flex-direction: column;
  padding: 10px 20rpx 0 20rpx;
}
.busline--item{
  display: flex;
  padding: 20rpx 0;
  background: white;
  border-radius: 5px;
  margin: 5px 0;
}
.busline--item--content{
  padding: 10px 20px 0;
  display: flex;
  flex-direction: column;
 
}
.busline--item--sensing{
  display: flex;
  margin-left: auto;
  align-items: center;
  padding-right: 20px;
}