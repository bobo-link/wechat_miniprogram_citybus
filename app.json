{
  "resolveAlias": {
    "~/*": "/*"
  },
  "useExtendedLib": {
    "kbone": true,
    "weui": true
  },
  "pages": [
    "pages/index/index",
    "pages/logs/logs",
    "pages/search/search",
    "pages/practice/contact/contact",
    "pages/weather/weather",
    "pages/practice/local_life/local_life",
    "pages/practice/shoplist/shoplist",
    "pages/forecasts/forecasts",
    "pages/buslinelist/buslinelist",
    "pages/setting/setting",
    "pages/login/login",
    "pages/transit/transit",
    "pages/transit_detail/transit_detail",
    "pages/transit_list/transit_list",
    "pages/route_map/route_map",
    "pages/busline_detail/busline_detail"
  ],
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "requiredPrivateInfos": [
    "getLocation",
    "chooseLocation"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "Weixin",
    "navigationBarTextStyle": "black"
  },
  "usingComponents": {
    "navigationBar": "Components/navigationBar/navigationBar",
    "i-swipeout": "Components/dist/swipeout/index",
    "van-empty": "@vant/weapp/empty/index",
    "van-action-sheet": "@vant/weapp/action-sheet/index",
    "iconfont": "/Components/iconfont/iconfont",
    "van-button": "@vant/weapp/button/index"
  },
  "tabBar": {
    "color": "#000",
    "selectedColor": "#4db900",
    "backgroundColor": "#fbfbfd",
    "position": "bottom",
    "list": [
      {
        "pagePath": "pages/search/search",
        "text": "Map",
        "iconPath": "img/tabbar_icon/map.png",
        "selectedIconPath": "img/tabbar_icon/map_active.png"
      },
      {
        "pagePath": "pages/index/index",
        "text": "Index",
        "iconPath": "img/tabbar_icon/index.png",
        "selectedIconPath": "img/tabbar_icon/index_active.png"
      },
      {
        "pagePath": "pages/transit/transit",
        "text": "Transit",
        "iconPath": "img/tabbar_icon/transit.png",
        "selectedIconPath": "img/tabbar_icon/transit_active.png"
      },
      {
        "pagePath": "pages/weather/weather",
        "text": "Weather",
        "iconPath": "img/tabbar_icon/weather.png",
        "selectedIconPath": "img/tabbar_icon/weather_active.png"
      },
      {
        "pagePath": "pages/setting/setting",
        "text": "Setting",
        "iconPath": "img/tabbar_icon/setting.png",
        "selectedIconPath": "img/tabbar_icon/setting_active.png"
      }
    ]
  },
  "networkTimeout": {
    "request": 4000,
    "downloadFile": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000
  },
  "sitemapLocation": "sitemap.json"
}